{
    "rules": {
        "no-array-mutation": false,
        "no-class": false,
        "no-delete": true,
        "no-expression-statement": false,
        "no-if-statement": false,
        "no-let": false,
        "no-loop-statement": false,
        "no-method-signature": false,
        "no-mixed-interface": false,
        "no-object-mutation": false,
        "no-reject": true,
        "no-this": false,
        "no-throw": false,
        "no-try": true,
        "readonly-array": [
            true,
            "ignore-local"
        ],
        "readonly-keyword": [
            true,
            "ignore-class"
        ]
    }
}
